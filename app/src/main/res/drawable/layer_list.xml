        <shape>
            <corners android:radius="6dp" />

            <solid android:color="#F2F1F4" />

        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:clipOrientation="horizontal">
            <shape>
                <corners android:radius="6dp" />

                <solid android:color="@color/colorAccent_8CC341" />
            </shape>
        </clip>
    </item>

</layer-list>
